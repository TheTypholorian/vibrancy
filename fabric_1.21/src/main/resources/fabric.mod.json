{
  "schemaVersion": 1,
  "id": "vibrancy",
  "version": "${version}",
  "name": "Vibrancy",
  "description": "A visuals mod that keeps the vanilla feel, adding colored lights and basic raytraced shadows",
  "authors": [
    "The Typhothanian"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/vibrancy/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.typho.vibrancy.VibrancyFabric"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": "${minecraft_version}",
    "sodium": "*"
  },
  "accessWidener": "vibrancy.accesswidener"
}
